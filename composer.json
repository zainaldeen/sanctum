{
    "name": "zainaldeen/sanctum",
    "description": "Provide refresh token for Laravel Sanctum wich provides a featherweight authentication system for SPAs and simple APIs.",
    "keywords": ["laravel", "sanctum", "auth", "refresh token"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel/sanctum/issues",
        "source": "https://github.com/laravel/sanctum"
    },
    "authors": [
        {
            "name": "Zain Aldeen Fayod",
            "email": "zainaldeenfayod@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "illuminate/contracts": "^6.9|^7.0|^8.0",
        "illuminate/database": "^6.9|^7.0|^8.0",
        "illuminate/support": "^6.9|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.3"
    },
    "autoload": {
        "psr-4": {
            "Zainaldeen\\Sanctum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zainaldeen\\Sanctum\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zainaldeen\\Sanctum\\SanctumServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
